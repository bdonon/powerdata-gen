{
  "default_net_path": "standard_cases/case60nordic_condenser_medium.json",
  "dataset_name": "data/case60nordic_condenser_medium",
  "n_train": 1000,
  "n_test": 1000,
  "seed": 2022,
  "sampling": {
    "topology": {
      "sampling_method": "random_disconnection",
      "params": {
        "gen": [1.0],
        "line": [0.34, 0.33, 0.33],
        "load": [1.0]
      },
      "black_lists": {
        "line": [25, 30, 32, 34, 36]
      }
    },
    "total_load": {
      "sampling_method": "uniform_factor",
      "params": [
        0.5,
        1.2
      ]
    },
    "active_load": {
      "sampling_method": "uniform_independent_values",
      "params": [
        1.0
      ]
    },
    "reactive_load": {
      "sampling_method": "uniform_power_factor",
      "params": [0.8, 1.0, 0.1]
    },
    "active_gen": {
      "sampling_method": "opf",
      "params": [65.0]
    },
    "voltage_setpoint": {
      "sampling_method": "constant",
      "params": []
    }
  }
}