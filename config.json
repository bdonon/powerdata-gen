{
  "default_net_path": "standard_cases/case60nordic.json",
  "dataset_name": "data/case60_simple_topo",
  "n_train": 10000,
  "n_test": 1000,
  "sampling": {
    "topology": {
      "sampling_method": "random_disconnection",
      "params": {
        "gen": [0.34, 0.33, 0.33],
        "line": [0.34, 0.33, 0.33]
      }
    },
    "total_load": {
      "sampling_method": "uniform_factor",
      "params": [
        0.5,
        1.2
      ]
    },
    "active_load": {
      "sampling_method": "normal_independent_factor",
      "params": [
        0.0,
        0.01
      ]
    },
    "reactive_load": {
      "sampling_method": "constant_pq_ratio",
      "params": []
    },
    "active_gen": {
      "sampling_method": "normal_independent_factor",
      "params": [
        0.0,
        0.01
      ]
    },
    "voltage_setpoint": {
      "sampling_method": "constant",
      "params": []
    }
  }
}